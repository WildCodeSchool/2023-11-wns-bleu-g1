		"eslint-config-next": "^14.2.2",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.31",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	},
	"resolutions": {
		"@radix-ui/react-focus-scope": "1.0.4"
	}
}
